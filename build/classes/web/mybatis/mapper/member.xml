<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">
	<!-- id로 mvo 검색하는 쿼리 -->
	<select id="idToMvo" parameterType="String" resultType="web.mybatis.vo.MemberVO">
		SELECT *
		FROM member
		WHERE u_id = #{u_id}
	</select>
</mapper>